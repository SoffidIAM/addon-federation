				<grid dataPath="/model:/atribut" fixedLayout="true" sclass="noBorderGrid">
					<columns>
						<column label="${c:l('federa_atribut.zul.Name-2') }" width="15%"></column>
						<column label="${c:l('federa_atribut.zul.ShortName-2') }"  width="15%"></column>
						<column label="${c:l('federa_atribut.zul.Oid') }"  width="15%"></column>
						<column label="${c:l('federa_atribut.zul.openidName') }"  width="15%"></column>
						<column label="${c:l('federacio.zul.value') }" ></column>
						<column label="-"  width="32px"></column>
					</columns>
					<datarow>
						<textbox bind="name" style="width:95%; margin-left: 8px; margn-right:8px" onChange=""></textbox>
						<textbox bind="shortName" style="width:95%; margin-left: 8px; margn-right:8px"  onChange=""></textbox>
						<textbox bind="oid" style="width:95%; margin-left: 8px; margn-right:8px"  onChange=""></textbox>
						<textbox bind="openidName" style="width:95%; margin-left: 8px; margn-right:8px" onChange=""></textbox>
						<hbox widths="*,30px" width="100%">
							<textbox bind="value" onChange="" multiline="true" rows="1"
								style="width:95%; margin-left: 8px; margn-right:8px">
							</textbox>
							<imageclic src="/img/pencil.png" width="24px">
								<attribute name="onClick"><![CDATA[
									    Events.sendEvent(new Event ("onEdit", 
								    		desktop.getPage("editor").getFellow("top"),
								    		new Object[] {
											    event.getTarget().getPreviousSibling(),
											    new com.soffid.iam.addons.federation.web.ScriptEnviroment().getSystemVars(self)
											}
									    ));
								]]></attribute>
							</imageclic>
						</hbox>
						<imageclic src="~./img/list-remove.gif" visible="${canDeleteFI}">
							<attribute name="onClick"><![CDATA[
								if (canDeleteFI) {
									String att = es.caib.zkib.datasource.XPathUtils.getValue(self, "name");
									final es.caib.zkib.binder.BindContext ctx = es.caib.zkib.datasource.XPathUtils.getComponentContext(self);
									Missatgebox.confirmaOK_CANCEL(String.format(org.zkoss.util.resource.Labels.getLabel("federacio.zul.confirmRemoveAttribute"), new Object [] {att}),
											org.zkoss.util.resource.Labels.getLabel("federacio.Borra"),
											new EventListener() {
												public void onEvent(Event evt) {
													if ("onOK".equals(evt.getName())) {
														es.caib.zkib.datasource.XPathUtils.removePath(ctx.getDataSource(), ctx.getXPath());
													}
											    }
											});															
								}
								
							]]>
							</attribute>
						</imageclic>
					</datarow>
				</grid> 
			</tabpanel>
		</tabpanels>
	</tabbox>
	
	<window id="pkcs12window" visible="false" closable="true" title="${c:l('federacio.zul.loadPkcs12') }">
		<label value="PIN:" width="10em" style="margin-left: 15px;"></label>
		<textbox type="password" id="pkcs12pin"></textbox>
		<attribute name="onClose">
			pkcs12window.setVisible(false);
			event.stopPropagation();
		</attribute>
		<fileupload onUpload="parsePkcs12(event.media)" onClose="pkcs12window.setVisible(false);" />
	</window>
	
	<window id="adaptiveAuthentication" visible="false" closable="close" title="${c:l('federacio.zul.adaptiveAuthentication') }"
		width="700px">
		<grid dataPath="/esquema/lista/treebox:/federationMember/extendedAuthenticationMethods" id="grid">
			<datarow>
				<div>
					<hbox width="100%" widths="150px,*">
						<div>
							<imageclic src="/addon/federation/img/up.png" width="16px">
								<attribute name="onClick"><![CDATA[
								{
									AuthenticationMethod o = es.caib.zkib.datasource.XPathUtils.getValue(self, ".");
									LinkedList l = es.caib.zkib.datasource.XPathUtils.getValue(grid, ".");
									index = l.indexOf(o);
									if (index > 0 )
									{
										l.remove(o);
										l.add(index-1, o);
										es.caib.zkib.binder.BindContext c = es.caib.zkib.datasource.XPathUtils.getComponentContext(grid);
										c.getDataSource().sendEvent(new es.caib.zkib.events.XPathRerunEvent(c.getDataSource(), c.getXPath()));
									}
								}
								]]></attribute>
							</imageclic>
							<imageclic src="/addon/federation/img/down.png"  width="16px">
								<attribute name="onClick"><![CDATA[
								{
									AuthenticationMethod o = es.caib.zkib.datasource.XPathUtils.getValue(self, ".");
									LinkedList l = es.caib.zkib.datasource.XPathUtils.getValue(grid, ".");
									index = l.indexOf(o);
									if (index +1 < l.size() )
									{
										l.remove(o);
										l.add(index+1, o);
										es.caib.zkib.binder.BindContext c = es.caib.zkib.datasource.XPathUtils.getComponentContext(grid);
										c.getDataSource().sendEvent(new es.caib.zkib.events.XPathRerunEvent(c.getDataSource(), c.getXPath()));
									}
								}
								]]></attribute>
							</imageclic>
							<imageclic src="~./img/list-remove.gif"  width="16px">
								<attribute name="onClick"><![CDATA[
								{
									AuthenticationMethod o = es.caib.zkib.datasource.XPathUtils.getValue(self, ".");
									LinkedList l = es.caib.zkib.datasource.XPathUtils.getValue(grid, ".");
									index = l.remove(o);
									es.caib.zkib.binder.BindContext c = es.caib.zkib.datasource.XPathUtils.getComponentContext(grid);
									c.getDataSource().sendEvent(new es.caib.zkib.events.XPathRerunEvent(c.getDataSource(), c.getXPath()));
								}
								]]></attribute>
							</imageclic>
							<input_etiqueta value="${c:l('federacio.zul.description')}"/>
